<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hubu.baby.mapper.QuestionCategoryMapper">

	<!-- 通用查询映射结果 -->
	<resultMap id="BaseResultMap" type="com.hubu.baby.entity.QuestionCategory">
		<id column="question_category_id" property="questionCategoryId" />
		<result column="question_category_name" property="questionCategoryName" />
		<result column="description" property="description" />
		<result column="status" property="status" />
		<result column="created" property="created" />
		<result column="updated" property="updated" />
	</resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        question_category_id AS questionCategoryId, question_category_name AS questionCategoryName, description, status, created, updated
    </sql>

</mapper>
