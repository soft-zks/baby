<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hubu.baby.mapper.AdContentMapper">

	<!-- 通用查询映射结果 -->
	<resultMap id="BaseResultMap" type="com.hubu.baby.entity.AdContent">
		<id column="ad_content_id" property="adContentId" />
		<result column="ad_category_id" property="adCategoryId" />
		<result column="title" property="title" />
		<result column="sub_title" property="subTitle" />
		<result column="title_desc" property="titleDesc" />
		<result column="url" property="url" />
		<result column="picture" property="picture" />
		<result column="content" property="content" />
		<result column="created" property="created" />
		<result column="updated" property="updated" />
	</resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        ad_content_id AS adContentId, ad_category_id AS adCategoryId, title, sub_title AS subTitle, title_desc AS titleDesc, url, picture, content, created, updated
    </sql>

</mapper>
